{
  "name": "kakao-chatbot-templates",
  "version": "0.1.4",
  "description": "For It is a package to use Kakao OpenBuilder's skill template more easily.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prepublish": "tsc",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/winz87/kakao-chatbot-templates.git"
  },
  "keywords": [
    "kakao-chatbot"
  ],
  "author": "jhkang <winzlove@naver.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/winz87/kakao-chatbot-templates/issues"
  },
  "homepage": "https://github.com/winz87/kakao-chatbot-templates#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/pupa": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.167",
    "lodash": "^4.17.20",
    "pupa": "^2.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
